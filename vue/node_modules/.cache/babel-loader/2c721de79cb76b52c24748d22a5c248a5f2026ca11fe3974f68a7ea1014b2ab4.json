el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"地　址\" >\r\n        <el-input v-model=\"form.uaddress\" autocomplete=\"off\" :disabled=\"change\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <div slot=\"footer\" class=\"dialog-footer\" style=\"text-align: center\">\r\n      <el-button style=\"margin-left: 100px\" @click=\"$router.push('/')\">取 消</el-button>\r\n\r\n      <el-popconfirm title=\"确认修改您的个人信息吗？\">\r\n        <el-button  style=\"margin-left: 10px\" slot=\"reference\" type=\"primary\" @confirm=\"changeInfo\" :disabled=\"!change\">修 改</el-button>\r\n      </el-popconfirm>\r\n      <el-button style=\"float: right\" type=\"success\" plain :disabled=\"!change\">提 交</el-button>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"Person\",\r\n  data(){\r\n    return{\r\n      form:{},\r\n      user:localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")):{},\r\n      change:true,\r\n    }\r\n  },\r\n  created() {\r\n    this.request.get(\"/user/selectById/\"+this.user.uid).then(res =>{\r\n      if(res.code ===\"200\"){\r\n        this.form=res.data\r\n      }\r\n    })\r\n  },\r\n  methods:{\r\n    save(){\r\n      this.request.post(\"/user\",this.form).then(res =>{\r\n        if(res){\r\n          this.$message.success(\"保存成功\")\r\n        }else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    changeInfo(){\r\n      this.change=false\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}